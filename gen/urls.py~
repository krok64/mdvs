    url(r'^type/$', 'show_Type',name='show_Type'),
    url(r'^type/(?P<id>\d+)/$', 'show_Type',name='show_Type'),
    url(r'^type/(?P<id>\d+)/ed/$', 'show_TypeEd',name='show_TypeEd'),
    url(r'^tu/$', 'show_TU',name='show_TU'),
    url(r'^tu/(?P<id>\d+)/$', 'show_TU',name='show_TU'),
    url(r'^tu/(?P<id>\d+)/ed/$', 'show_TUEd',name='show_TUEd'),
    url(r'^item/$', 'show_Item',name='show_Item'),
    url(r'^item/(?P<id>\d+)/$', 'show_Item',name='show_Item'),
